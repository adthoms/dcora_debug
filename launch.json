{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "dcora-tests",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/dcora/build/bin/testDCORA",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/dcora/build/",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
        {
          "description": "Explicitly load the binary",
          "text": "file ${workspaceFolder}/dcora/build/bin/testDCORA",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "build",
      "logging": {
        "trace": true,
        "engineLogging": true,
        "traceResponse": true,
        "programOutput": true,
        "exceptions": true
      }
    },
